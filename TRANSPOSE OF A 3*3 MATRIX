#include<stdio.h>
void main()
{
    int i,j,a[4][4],b[4][4];
    for(i=0;i<4;i++)
        for(j=0;j<4;j++)
        {
            printf("enter data of A[%d][%d]",i+1,j+1);
            scanf("%d",&a[i][j]);
        }
    printf("the transpose of matrix A\n");
    for(i=0;i<4;i++)
        for(j=0;j<4;j++)
        {

            b[i][j]=a[j][i];
            printf("%d  ",b[i][j]);
            if (j==3)
                printf("\n");
        }







}
